			}
		},
		{
			"path": "pages/host/joinSubRoom",
			"style": {
				"navigationBarTitleText": "跨房间连麦",
				"enablePullDownRefresh": false
			}
		}
	],
	"globalStyle": {
		"navigationBarTextStyle": "white",
		"navigationBarTitleText": "RTC-RN-TEST-DEMO",
		"navigationBarBackgroundColor": "#007AFF",
		"backgroundColor": "#F8F8F8",
		"navigationBarShadow": {
			"colorType": "grey"
		},
		"app-plus": {
			"titleNView": {
				"backButton": { //自定义 backButton
					"background": "#FFFFFF"
