			}

		},
		{
			"path": "pages/host/cdnConfig",
			"style": {
				"navigationBarTitleText": "配置CDN",
				"enablePullDownRefresh": false
			}
		},
		{
			"path": "pages/messagePanel",
			"style": {
				"navigationBarTitleText": "消息面板",
				"enablePullDownRefresh": false
			}
		},
		{
			"path": "pages/meeting/audioEffect",
			"style": {
				"navigationBarTitleText": "音效面板",
				"enablePullDownRefresh": false
			}
		},
		{
			"path": "pages/meeting/audioMix",
