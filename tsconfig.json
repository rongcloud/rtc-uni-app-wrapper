{
    "compilerOptions": {
      "target": "esnext",
      "lib": ["ESNext", "DOM"],
      "module": "esnext",
      "declaration": true,
      "outDir": "dist",
      "rootDir": "src",
      "strict": true,
      "removeComments": false,
    },
    "include": ["src"],
    "exclude": ["node_modules"],
    "typedocOptions": {
        "exclude": "src/uni.d.ts",
        "out": "build/docs",
        "includeVersion": true,
        "readme": "README-Doc.md",
    }
  }